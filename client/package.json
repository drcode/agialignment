{
  "name": "agialignment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@lisperati/super-client": "file:../../super-client",
    "@mui/material": "^5.10.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "framer-motion": "^6.3.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^14.0.0",
    "react-scripts": "5.0.1",
    "relay-runtime": "^14.0.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run relay && react-scripts build && mv build ../resources && rm -rf ../resources/public && mv ../resources/build ../resources/public && rm -rf ../resources/public/avatars && cd ../resources/public && ln -s ../avatars avatars",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-loop-func": "off",
      "jsx-a11y/anchor-is-valid": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-relay": "^14.0.0",
    "relay-compiler": "^14.0.0"
  },
  "relay": {
    "language": "javascript",
    "src": "./src/",
    "schema": "./schema.graphql",
    "schemaExtensions": [
      "./src/"
    ]
  }
}
